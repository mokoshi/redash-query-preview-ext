{
  "manifest_version": 3,
  "name": "Redash Query Preview",
  "version": "1.1",
  "description": "Redash の Query Result のプレビューを表示します",
  "permissions": [],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/index.js"],
      "css": ["content/styles.css"],
      "world": "MAIN"
    }
  ],
  "action": {
    "default_popup": "popup/index.html",
    "default_icon": {
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  },
  "icons": {
    "48": "images/icon48.png",
    "128": "images/icon128.png"
  }
}
